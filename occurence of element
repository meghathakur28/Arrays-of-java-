import java.util.*;
public class problem_array {
    public static int occurence(int[] arr,int n,int ele)
    {
        int count=0;
           for(int i=0;i<n;i++)
           {
               if(arr[i]==ele)
               {
                count++;
               }
           }
           return count;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the value of n: ");
        int n=sc.nextInt();
        int[] arr= new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        System.out.println("enter the element u want to check the occurnce: ");
        int ele=sc.nextInt();
        System.out.println(occurence(arr,n,ele));
    }
    
}
